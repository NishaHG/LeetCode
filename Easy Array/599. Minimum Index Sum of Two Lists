class Solution:
    def findRestaurant(self, list1: List[str], list2: List[str]) -> List[str]:
        res=[]
        min_sum=float('inf')
        index_map={word:i for i,word in enumerate(list1)}

        for j,word in enumerate(list2):
            if word in index_map:
                total=j+index_map[word]
                if total<min_sum:
                    min_sum=total
                    res=[word]
                elif total==min_sum:
                    res.append(word)
        return res



