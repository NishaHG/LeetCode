class Solution:
    def findWords(self, words: List[str]) -> List[str]:
        result=[]
        row1=set("qwertyuiop")
        row2=set("asdfglkjh")
        row3=set("zxcvmnb")

        for word in words:
            lower_word=set(word.lower())
            if lower_word<=row1 or lower_word<=row2 or lower_word<=row3 :
                result.append(word)
        return result        
