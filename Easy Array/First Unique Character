class Solution:
    def firstUniqChar(self, s: str) -> int:
        s_dict={}

        for ch in s:
            if ch  in s_dict:
                s_dict[ch]+=1
            else:
                s_dict[ch]=1

        for i in range(len(s)):
            if s_dict[s[i]]==1:
                return i

        return -1


