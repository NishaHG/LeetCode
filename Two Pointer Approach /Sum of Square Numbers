class Solution:
    def judgeSquareSum(self, c: int) -> bool:
        # Two Pointer 

        a=0
        b=math.isqrt(c)

        while a<=b:
            s=a*a+b*b
            if s==c:
                return True
            elif s<=c:
                a+=1
            else:
                b-=1
        return False

        
