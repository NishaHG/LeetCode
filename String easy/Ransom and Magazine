####  1    :  
without using built in function

class Solution:
    def canConstruct(self, ransomNote: str, magazine: str) -> bool:
        magazine_count={}
        for ch in magazine:
            if ch in magazine_count:
                magazine_count[ch]+=1
            else:
                magazine_count[ch]=1

        for ch in ransomNote:
            if ch not in magazine_count or magazine_count[ch]==0:
                return False
            magazine_count[ch]-=1
        
        return True

####  2

from collections import Counter

def canConstruct(ransomNote, magazine):
    ransom_count = Counter(ransomNote)   # Count letters in ransomNote
    magazine_count = Counter(magazine)   # Count letters in magazine
    
    for char, count in ransom_count.items():
        if magazine_count[char] < count:  # Not enough of that letter
            return False
    return True

# Example
print(canConstruct("aa", "aab"))  # True
print(canConstruct("aab", "ab"))  # False

