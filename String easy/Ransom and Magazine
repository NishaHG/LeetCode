class Solution:
    def canConstruct(self, ransomNote: str, magazine: str) -> bool:
        magazine_count={}
        for ch in magazine:
            if ch in magazine_count:
                magazine_count[ch]+=1
            else:
                magazine_count[ch]=1

        for ch in ransomNote:
            if ch not in magazine_count or magazine_count[ch]==0:
                return False
            magazine_count[ch]-=1
        
        return True

        
